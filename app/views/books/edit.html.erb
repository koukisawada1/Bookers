<h1>Editing Book</h1>

<%= render 'layouts/error_messages', model: @book %>

<%= form_for(@book) do |f| %>
    <h4>タイトル</h4>
    <%= f.text_field :title %>
    <h4>本文</h4>
    <%= f.text_area :body %>
    <%= f.submit 'Update Book' %>
<% end %>
 	<%= link_to "show", book_path(@book.id) %>
	<%= link_to "Back", books_path %>
